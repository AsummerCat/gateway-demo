spring:
  application:
    name: gateway-api-rureka
  cloud:
    gateway:
      discovery:
        locator:
          enabled: true
      routes:
        - id: fsh-house
          uri: lb://fsh-house
          predicates:
            - Path=/house/**
server:
  port: 8091

eureka:
  client:
    serviceUrl:
      defaultZone: http://localhost:8100/eureka
